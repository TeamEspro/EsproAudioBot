{
  "name": "yt-downloader-bot",
  "description": "A simple Telegram bot to download YouTube audio and video.",
  "repository": "https://github.com/TeamEspro/EsproAudioBot",
  "logo": "https://link-to-your-logo.png",
  "keywords": ["telegram", "bot", "youtube", "downloader"],
  "env": {
    "API_ID": {
      "description": "Your Telegram API ID.",
